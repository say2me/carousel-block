{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "xwp-blocks/xwp-carousel",
	"version": "0.1.0",
	"title": "Carousel",
	"category": "widgets",
	"icon": "smiley",
	"description": "Carousel container",
	"supports": {
		"html": false,
		"className": false
	},
	"attributes": {
        "autoplay": {
            "type": "string",
			"source": "attribute",
			"selector": "div[data-xwp-carousel-autoplay]",
			"attribute": "data-xwp-carousel-autoplay",
			"default": "false"
		},
		"slides_count": {
			"type": "number",
			"default": 0
		}
	},
	"textdomain": "carousel",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
